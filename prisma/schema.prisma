generator client {
  provider = "prisma-client-js"
}

datasource db {
  // Switched to SQLite for local dev seeding. This is reversible.
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model MgnregaData {
  state      String
  district   String
  month      String
  households Int
  workdays   Int
  wages      Decimal

  @@id([state, district, month])
  @@map("mgnrega_data")
}
